        "inputSchema": "{\n  \"candidate_strengths\": [\n    \"string\",\n    \"string\"\n  ],\n  \"candidate_weaknesses\": [\n    \"string\",\n    \"string\"\n  ],\n  \"risk_factor\": {\n    \"score\": \"Low | Medium | High\",\n    \"explanation\": \"string\"\n  },\n  \"reward_factor\": {\n    \"score\": \"Low | Medium | High\",\n    \"explanation\": \"string\",\n    \"time_horizon_fit\": \"Short-term | Long-term | Both\"\n  },\n  \"overall_fit_rating\": 0,\n  \"dimension_wise_scoring\": {\n    \"skills_match\": {\n      \"score\": 0,\n      \"out_of\": 10,\n      \"reason\": \"string\"\n    },\n    \"experience_relevance\": {\n      \"score\": 0,\n      \"out_of\": 10,\n      \"reason\": \"string\"\n    },\n    \"role_and_domain_fit\": {\n      \"score\": 0,\n      \"out_of\": 10,\n      \"reason\": \"string\"\n    },\n    \"seniority_alignment\": {\n      \"score\": 0,\n      \"out_of\": 10,\n      \"reason\": \"string\"\n    },\n    \"red_flags_and_gaps\": {\n      \"score\": 0,\n      \"out_of\": 10,\n      \"reason\": \"string\"\n    }\n  },\n  \"weighted_score_calculation\": {\n    \"formula\": \"(Skills * 0.30) + (Experience * 0.25) + (Role Fit * 0.20) + (Seniority * 0.15) + (Red Flags * 0.10)\",\n    \"calculation\": {\n      \"skills\": 0,\n      \"experience\": 0,\n      \"role_fit\": 0,\n      \"seniority\": 0,\n      \"red_flags\": 0\n    },\n    \"weighted_components\": {\n      \"skills_component\": 0.0,\n      \"experience_component\": 0.0,\n      \"role_fit_component\": 0.0,\n      \"seniority_component\": 0.0,\n      \"red_flags_component\": 0.0\n    },\n    \"total_weighted_score\": 0.0\n  },\n  \"justification_for_rating\": \"string\"\n}\n",
